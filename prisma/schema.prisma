datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

model PrintJob {
  jobId          String    @id @default(uuid())
  escPosCommands Bytes
  printed        Boolean   @default(false)
  submitted      DateTime  @default(now())
  printedAt      DateTime?
}